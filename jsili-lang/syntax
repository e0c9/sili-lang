// 1 - (2 * 3) < 4 == false

expression -> literal
            | unary
            | binary
            | grouping ;

literal    -> NUMBER | STRING | "true" | "false" | "sili";

grouping   -> "(" expression ")" ;

unary      -> ( "-" | "!" ) expression ;

binary     -> expression operator expression ;

operator   -> "==" | "!=" | "<" | "<=" | ">" | ">="
             | "+" | "-" | "*" | "/" ;